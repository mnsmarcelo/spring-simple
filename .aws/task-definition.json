{
  "containerDefinitions": [
    {
      "name": "spring-app",
      "image": "651100910218.dkr.ecr.us-east-2.amazonaws.com/spring-app-ecr:latest",
      "cpu": 0,
      "portMappings": [
        {
          "name": "spring-app-8080-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "family": "spring-task-def",
  "taskRoleArn": "arn:aws:iam::651100910218:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::651100910218:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024"
}